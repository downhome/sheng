<w:document xmlns:w="http://schemas.openxmlformats.org/wordprocessingml/2006/main">
  <w:p>
    <w:r>
      <w:t xml:space="preserve">First Name: </w:t>
    </w:r>
    <w:fldSimple w:instr=" MERGEFIELD  person.first_name \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«person.first_name»</w:t>
      </w:r>
    </w:fldSimple>
    <w:r>
      <w:tab/>
      <w:t xml:space="preserve">Last Name: </w:t>
    </w:r>
    <w:fldSimple w:instr=" MERGEFIELD person.last_name | upcase \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«person.last_name | upcase»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:fldSimple w:instr=" MERGEFIELD start:person.socks \* MERGEFORMAT ">
      <w:r w:rsidR="00455C8A">
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«start:person.socks»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:fldSimple w:instr=" MERGEFIELD color \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«color»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:fldSimple w:instr=" MERGEFIELD size \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«size»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:r>
      <w:t xml:space="preserve">Sock Area: </w:t>
    </w:r>
    <w:fldSimple w:instr=" MERGEFIELD dimensions.width * dimensions.height \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«dimensions.width * dimensions.height»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:r>
      <w:t xml:space="preserve">Sock Circumference: </w:t>
    </w:r>
    <w:fldSimple w:instr=" MERGEFIELD (dimensions.width * 2) + (dimensions.height * 2) \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«(dimensions.width * 2) + (dimensions.height * 2)»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:p>
    <w:fldSimple w:instr=" MERGEFIELD end:person.socks \* MERGEFORMAT ">
      <w:r w:rsidR="00455C8A">
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«end:person.socks»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
  <w:ffData>
    <w:name w:val="veggies.green.spinach"/>
    <w:enabled/>
    <w:calcOnExit w:val="0"/>
    <w:checkBox>
      <w:sizeAuto/>
      <w:default w:val="0"/>
    </w:checkBox>
  </w:ffData>
  <w:p>
    <w:r>
      <w:t xml:space="preserve">First Name Again: </w:t>
    </w:r>
    <w:fldSimple w:instr=" MERGEFIELD  person.first_name \* MERGEFORMAT ">
      <w:r>
        <w:rPr>
          <w:noProof/>
        </w:rPr>
        <w:t>«person.first_name»</w:t>
      </w:r>
    </w:fldSimple>
  </w:p>
</w:document>
